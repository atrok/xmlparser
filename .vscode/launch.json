    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Attach by Process ID",
            "processId": "${command:PickProcess}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Parser",
            "program": "${workspaceFolder}/parser.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Async test",
            "program": "${workspaceFolder}/asynctest.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Async Multi",
            "program": "${workspaceFolder}/asyncqueuemulti.js"
        }

    ]
}